{
    "swagger":"2.0",
    "info":{
       "title":"NodeJS Test Upwork with Swagger",
       "description":"This is a sample NodeJS Test Upwork with Swagger",
       "version":"1.0"
    },
    "paths":{
       "/api_v1/users":{
          "get":{
             "x-swagger-router-controller":"users",
             "operationId":"getUser",
             "responses":{
                "default":{
                   "description":"Unexpected error",
                   "schema":{
                      "$ref":"#/definitions/Error"
                   }
                },
                "200":{
                   "description":"Successful request.",
                   "schema":{
                      "$ref":"#/definitions/getUser"
                   }
                }
             }
          },
          "post":{
             "x-swagger-router-controller":"users",
             "operationId":"postUser",
             "parameters":[
                {
                   "name":"users",
                   "in":"body",
                   "description":"Create User",
                   "required":true,
                   "schema":{
                      "$ref":"#/definitions/postUser"
                   }
                }
             ],
             "responses":{
                "default":{
                   "description":"Unexpected error",
                   "schema":{
                      "$ref":"#/definitions/Error"
                   }
                },
                "400":{
                   "description":"Missing Parameter",
                   "schema":{
                      "$ref":"#/definitions/Error400"
                   }
                },
                "200":{
                   "description":"User Created"
                }
             }
          }
       }
    },
    "definitions":{
        "Error":{
            "required":[
               "code",
               "message"
            ],
            "properties":{
               "code":{
                  "type":"integer",
                  "format":"int32"
               },
               "message":{
                  "type":"string"
               }
            }
         },
         "Error400":{
            "required":[
               "message",
               "description"
            ],
            "properties":{
               "message":{
                  "type":"string"
               },
               "description":{
                  "type":"string"
               }
            }
         },
       "getUser":{
          "properties":{
              "status": { 
                "type":"string"
              },
              "data": {
                "type":"object",
                "properties":{
                    "id":{
                        "type":"integer",
                        "x-primary-key": true
                    },
                    "userid":{
                        "type":"integer"
                    },
                    "phoneNumber":{
                        "type":"string"
                    }
                }
            }
          }
       },
       "postUser":{
          "type":"object",
          "properties":{
             "userid":{
                "type":"string"
             },
             "phoneNumber":{
                "type":"string"
             }
          },
          "required":[
             "userid",
             "phoneNumber"
          ],
          "example":{
             "userid":"123",
             "phoneNumber":"911234567890"
          }
       },
       "patchUser":{
        "type":"object",
        "properties":{
           "userid":{
              "type":"string"
           },
           "phoneNumber":{
              "type":"string"
           }
        },
        "required":[
           "userid",
           "phoneNumber"
        ],
        "example":{
           "userid":"123",
           "phoneNumber":"911234567890"
        }
     }
    }
 }